{
    "package": {
        "name": "static",
        "repo": "fastobo-validator",
        "subject": "fastobo",
        "github_use_tag_release_notes": true,
        "github_release_notes_file": "CHANGELOG.md"
    },

    "version": {
        "name": "master",
        "desc": "Development version built from latest commit on master branch.",
        "released": "$DATE",
        "vcs_tag": "master",
        "attributes": [],
        "gpgSign": false
    },

    "files": [
      {
        "includePattern": "dist/(fastobo_validator-.*\.tar\.gz)",
        "uploadPattern": "/master/$1",
        "matrixParams": { "override": 1 },
        "list_in_downloads": true
      }
    ],

    "publish": true
}
