{
    "package": {
        "name": "static",
        "repo": "fastobo-validator",
        "subject": "fastobo",
        "github_use_tag_release_notes": true,
        "github_release_notes_file": "CHANGELOG.md"
    },

    "version": {
        "name": "stable",
        "desc": "Release version built from the latest release tag.",
        "released": "$DATE",
        "vcs_tag": "$TRAVIS_TAG",
        "attributes": [],
        "gpgSign": false
    },

    "files": [
      {
        "includePattern": "dist/(fastobo_validator-.*\.tar\.gz)",
        "uploadPattern": "/master/$1",
        "matrixParams": { "override": 1, "list_in_downloads": true }
      }
    ],

    "publish": true
}
