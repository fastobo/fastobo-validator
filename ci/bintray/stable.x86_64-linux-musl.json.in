{
    "package": {
        "name": "x86_64-linux-musl",
        "repo": "fastobo-validator",
        "subject": "fastobo",
        "github_use_tag_release_notes": true,
        "github_release_notes_file": "CHANGELOG.md"
    },

    "version": {
        "name": "stable",
        "desc": "Release version built from the lastest release tag.",
        "released": "$DATE",
        "vcs_tag": "$TRAVIS_TAG",
        "attributes": [],
        "gpgSign": false
    },

    "files": [
      {
        "includePattern": "dist/fastobo_validator-x86_64-linux-musl.tar.gz",
        "uploadPattern": "/stable/fastobo_validator-x86_64-linux-musl.tar.gz",
        "matrixParams": { "override": 1, "list_in_downloads": true }
      }
    ],

    "publish": true
}
