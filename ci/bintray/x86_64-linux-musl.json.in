{
    "package": {
        "name": "x86_64-linux-musl",
        "repo": "fastobo-validator",
        "subject": "fastobo",
        "github_use_tag_release_notes": true,
        "github_release_notes_file": "CHANGELOG.md"
    },

    "version": {
        "name": "$TRAVIS_BRANCH",
        "desc": "$DESC",
        "released": "$DATE",
        "vcs_tag": "$TAG",
        "attributes": [],
        "gpgSign": false
    },

    "files": [
        {
          "includePattern": "target/x86_64-unknown-linux-musl/release/fastobo-validator",
          "uploadPattern": "/fastobo-validator",
          "matrixParams": { "override": 1 }
        }
    ],

    "publish": true
}
